                <td class="tableRowBottom" bgcolor="{!If(cas.Engagement_Surveys__c == 'Partially Penetrated', '#00BFFF', 'white')}"><apex:outputText value="" rendered="{!If(cas.Engagement_Surveys__c == 'Partially Penetrated', true, false)}" /></td>
