            </tr>
            <tr>
                <td class="tableRowBottom">Best Employers</td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Best_Employers__c == 'Not Viable', 'black', 'white')}"><apex:outputText value="" rendered="{!If(cas.Best_Employers__c == 'Not Viable', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Best_Employers__c == 'Recent Loss', '#696969', 'white')}"><apex:outputText value="" rendered="{!If(cas.Best_Employers__c == 'Recent Loss', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Best_Employers__c == 'No Activity or N/A', '#FF0000', 'white')}"><apex:outputText value="" rendered="{!If(cas.Best_Employers__c == 'No Activity or N/A', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Best_Employers__c == 'Opportunity in SFDC', '#00FF00', 'white')}"><apex:outputText value="" rendered="{!If(cas.Best_Employers__c == 'Opportunity in SFDC', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Best_Employers__c == 'Partially Penetrated', '#00BFFF', 'white')}"><apex:outputText value="" rendered="{!If(cas.Best_Employers__c == 'Partially Penetrated', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Best_Employers__c == 'Lead Provider', '#4876FF', 'white')}"><apex:outputText value="" rendered="{!If(cas.Best_Employers__c == 'Lead Provider', true, false)}" /></td>
            </tr>
            <tr>
                <td class="tableRowBottom">Leadership Consulting</td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Leadership_Consulting__c == 'Not Viable', 'black', 'white')}"><apex:outputText value="" rendered="{!If(cas.Leadership_Consulting__c == 'Not Viable', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Leadership_Consulting__c == 'Recent Loss', '#696969', 'white')}"><apex:outputText value="" rendered="{!If(cas.Leadership_Consulting__c == 'Recent Loss', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Leadership_Consulting__c == 'No Activity or N/A', '#FF0000', 'white')}"><apex:outputText value="" rendered="{!If(cas.Leadership_Consulting__c == 'No Activity or N/A', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Leadership_Consulting__c == 'Opportunity in SFDC', '#00FF00', 'white')}"><apex:outputText value="" rendered="{!If(cas.Leadership_Consulting__c == 'Opportunity in SFDC', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Leadership_Consulting__c == 'Partially Penetrated', '#00BFFF', 'white')}"><apex:outputText value="" rendered="{!If(cas.Leadership_Consulting__c == 'Partially Penetrated', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Leadership_Consulting__c == 'Lead Provider', '#4876FF', 'white')}"><apex:outputText value="" rendered="{!If(cas.Leadership_Consulting__c == 'Lead Provider', true, false)}" /></td>
            </tr>
            <tr>
                <td class="tableRowBottom">Selection and Assessment</td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Selection_and_Assessment__c == 'Not Viable', 'black', 'white')}"><apex:outputText value="" rendered="{!If(cas.Selection_and_Assessment__c == 'Not Viable', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Selection_and_Assessment__c == 'Recent Loss', '#696969', 'white')}"><apex:outputText value="" rendered="{!If(cas.Selection_and_Assessment__c == 'Recent Loss', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Selection_and_Assessment__c == 'No Activity or N/A', '#FF0000', 'white')}"><apex:outputText value="" rendered="{!If(cas.Selection_and_Assessment__c == 'No Activity or N/A', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Selection_and_Assessment__c == 'Opportunity in SFDC', '#00FF00', 'white')}"><apex:outputText value="" rendered="{!If(cas.Selection_and_Assessment__c == 'Opportunity in SFDC', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Selection_and_Assessment__c == 'Partially Penetrated', '#00BFFF', 'white')}"><apex:outputText value="" rendered="{!If(cas.Selection_and_Assessment__c == 'Partially Penetrated', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Selection_and_Assessment__c == 'Lead Provider', '#4876FF', 'white')}"><apex:outputText value="" rendered="{!If(cas.Selection_and_Assessment__c == 'Lead Provider', true, false)}" /></td>
            </tr>
            <tr>
                <td class="tableRowBottom">HR Effectiveness</td>
                <td class="tableRowBottom" bgcolor="{!If(cas.HRE__c == 'Not Viable', 'black', 'white')}"><apex:outputText value="" rendered="{!If(cas.HRE__c == 'Not Viable', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.HRE__c == 'Recent Loss', '#696969', 'white')}"><apex:outputText value="" rendered="{!If(cas.HRE__c == 'Recent Loss', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.HRE__c == 'No Activity or N/A', '#FF0000', 'white')}"><apex:outputText value="" rendered="{!If(cas.HRE__c == 'No Activity or N/A', true, false)}" /></td>
