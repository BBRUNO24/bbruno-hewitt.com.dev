<apex:page cache="false" title="APAC Client Scorecard" standardController="Account" extensions="ctlr_Client_Scorecard_APAC" renderAs="PDF">
<apex:stylesheet value="{!URLFOR($Resource.Client_Scorecard, 'Client_Scorecard_StyleSheet.css')}" />
<head>
    <style type="text/css">
    @page {
        counter-increment: page;
        @bottom-right {
            font-family: Arial Sans-Serif;
            font-size: 9.0pt;
            font-style: italic;
            content: "Page " counter(page) " of " counter(pages) "&nbsp;&nbsp;&nbsp;" 
        }
        @bottom-center {
            font-family: Arial Sans-Serif;
            font-size: 9.0pt;
            font-style: italic;
            content: "Printed on &nbsp;<apex:outputText value="{0,date,MMMM dd, yyyy}"><apex:param value="{!today()}"/></apex:outputText>" 
        }
        @bottom-left {
            font-family: Arial Sans-Serif;
            font-size: 9.0pt;
            font-style: italic;
            content: "&nbsp;&nbsp;&nbsp;{!myScorecard.accountName}";
        }
    }
    </style>
</head>
<div class="container">
    <table class="table0">
        <tr>
            <td align="left"><apex:image url="{!URLFOR($Resource.Client_Scorecard, 'AonHewittLogo.PNG')}" width="100" height="27"/></td>
        </tr>
    </table>

    <div class="top">
        <table class="table0">
            <tr>
                <th align="left" class="title">{!financialYear} {!quarter} APAC Client Scorecard</th>
                <th align="right" class="title">&nbsp;</th>
            </tr>
        </table>
        <hr class="hr"/>
    </div>
    
    <div class="div1">
        <table class="table1">
        <tr>
            <th class="tableHeadGray" colspan="2">Account Summary</th>
        </tr>
        <tr>
            <td width="45%" class="tableRow">Account</td>
            <td width="55%" class="tableRow"><apex:outputText value="{!myScorecard.accountName}"/></td>
        </tr>
        <tr>
            <td class="tableRowBlue">Market</td>
            <td class="tableRowBlue"><apex:outputText value="{!myScorecard.market}"/></td>
        </tr>
        <tr>
            <td class="tableRow">Account Executive</td>
            <td class="tableRow"><apex:outputText value="{!myScorecard.ownerName}"/></td>
        </tr>
        <tr>
            <td class="tableRowBlue">Executive Sponsor</td>
            <td class="tableRowBlue"><apex:outputText value="{!myScorecard.executiveSponsor}"/></td>
        </tr>
        <apex:repeat var="iat" value="{!myScorecard.iatList}">
        <tr style="{!IF(iat.Contact_Role__c == 'General Manager (HRO GM)', 'display:visible;' , 'display:none')}">
            <td class="tableRow">GM</td>
            <td class="tableRow">{!iat.Contact__r.Name}</td>
        </tr>
        </apex:repeat>
        <apex:repeat var="iat" value="{!myScorecard.iatList}">
        <tr style="{!IF(iat.Contact_Role__c == 'BGM (Business Group Manager)', 'display:visible;' , 'display:none')}">  
            <td class="tableRowBlue">BGM</td>
            <td class="tableRowBlue">{!iat.Contact__r.Name}</td>
        </tr>
        </apex:repeat>
        <tr>
            <td class="tableRow">Client Promise Required</td>
            <td class="tableRow"><apex:outputText value="{!If(account.Client_Promise_Required__c, 'Yes', 'No')}"/></td>
        </tr>
        <tr>
            <td class="tableRowBlue">Client Promise Score</td>
            <td class="tableRowBlue"><apex:outputText value="{!score} % for {!currYear}"/></td>
        </tr>
        </table>
        <br/>
        
        <table class="table1a">
        <tr>
            <th class="tableHeadGray" colspan="2">Overall Relationship</th>
        </tr>
        <apex:repeat var="cas" value="{!myScorecard.casList}">
        <tr>
            <td class="tableRow" width="45%">Overall Status</td>
            <td class="tableRowRed" width="55%" style="{!If(cas.Overall_Relationship_Status__c == 'Red', 'display:visible;' , 'display:none')}">{!cas.Overall_Relationship_Status__c}</td>
            <td class="tableRowYellow" width="55%" style="{!If(cas.Overall_Relationship_Status__c == 'Yellow', 'display:visible;' , 'display:none')}">{!cas.Overall_Relationship_Status__c}</td>
            <td class="tableRowGreen" width="55%" style="{!If(cas.Overall_Relationship_Status__c == 'Green', 'display:visible;' , 'display:none')}">{!cas.Overall_Relationship_Status__c}</td>
        </tr>
        <tr>
            <td class="tableRowBlue">Consulting Status</td>
            <td class="tableRowRed" width="50%" style="{!If(cas.Consulting_Relationship_Status__c == 'Red', 'display:visible;' , 'display:none')}">{!cas.Consulting_Relationship_Status__c}</td>
            <td class="tableRowYellow" width="50%" style="{!If(cas.Consulting_Relationship_Status__c == 'Yellow', 'display:visible;' , 'display:none')}">{!cas.Consulting_Relationship_Status__c}</td>
            <td class="tableRowGreen" width="50%" style="{!If(cas.Consulting_Relationship_Status__c == 'Green', 'display:visible;' , 'display:none')}">{!cas.Consulting_Relationship_Status__c}</td>
        </tr>
        <tr>
            <td class="tableRow">Outsourcing Status</td>
            <td class="tableRowRed" width="50%" style="{!If(cas.Outsourcing_Relationship_Status__c == 'Red', 'display:visible;' , 'display:none')}">{!cas.Outsourcing_Relationship_Status__c}</td>
            <td class="tableRowYellow" width="50%" style="{!If(cas.Outsourcing_Relationship_Status__c == 'Yellow', 'display:visible;' , 'display:none')}">{!cas.Outsourcing_Relationship_Status__c}</td>
            <td class="tableRowGreen" width="50%" style="{!If(cas.Outsourcing_Relationship_Status__c == 'Green', 'display:visible;' , 'display:none')}">{!cas.Outsourcing_Relationship_Status__c}</td>
        </tr>
        <tr>
            <td class="tableRowBlue">Leverage ARS Relationship</td>
            <td class="tableRowBlue">{!cas.ARS_Relationship__c}</td>
        </tr>
        <tr>
            <td class="tableRow">Risks</td>
            <td class="tableRow">{!cas.Risks__c}</td>
        </tr>
        <tr>
            <td class="tableRowBlue">Opportunities</td>
            <td class="tableRowBlue">{!cas.Opportunities__c}</td>
        </tr>
        </apex:repeat>
        </table>
        <br/>
        
        <apex:dataTable value="{!myScorecard.winList}" var="opp" cellpadding="2" cellspacing="2" rowClasses="tableRow,tableRowBlue" styleClass="table3" headerClass="tableHeadGray">
            <apex:facet name="header"><apex:outputText value="Top 5 Recently Won Opportunities by First Year Value as of {0,date,MMMMM dd, yyyy}"><apex:param value="{!today()}"/></apex:outputText></apex:facet>
            <apex:column styleClass="tableRow" headerValue="Opportunity Name" headerClass="tableRow">
                <apex:outputText value="{!opp.Name}"/>
            </apex:column>
            <apex:column styleClass="tableRow" headerValue="Current Provider" headerClass="tableRow">
                <apex:outputText value="{!opp.Current_Provider_Reference__r.Name}"/>
            </apex:column>
            <apex:column styleClass="tableRowData" headerValue="FY Value" headerClass="tableRowData">
                <apex:outputText value="$ {0,number,###,###,###,###}"><apex:param value="{!opp.Total_First_Year_Value__c}"/></apex:outputText>
            </apex:column>
            <apex:column styleClass="tableRow" headerValue="Reason" headerClass="tableRow">
                <apex:outputText value="{!opp.Reason_Won_or_Lost__c}"/>
            </apex:column>
            <apex:column styleClass="tableRowData" headerValue="Opportunity Closed Date" headerClass="tableRowData">
                <apex:outputText value="{0,date,dd/MM/yy}"><apex:param value="{!opp.Opportunity_Closed_Date__c}"/></apex:outputText>
            </apex:column>
        </apex:dataTable>
        <br/>
        
        <apex:dataTable value="{!myScorecard.lostList}" var="opp" cellpadding="2" cellspacing="2" rowClasses="tableRow,tableRowBlue" styleClass="table3" headerClass="tableHeadGray">
            <apex:facet name="header"><apex:outputText value="Top 5 Recently Lost Opportunities by First Year Value as of {0,date,MMMMM dd, yyyy}"><apex:param value="{!today()}"/></apex:outputText></apex:facet>
            <apex:column styleClass="tableRow" headerValue="Opportunity Name" headerClass="tableRow">
                <apex:outputText value="{!opp.Name}"/>
            </apex:column>
            <apex:column styleClass="tableRow" headerValue="Chosen Provider" headerClass="tableRow">
                <apex:outputText value="{!opp.Chosen_Provider_Reference__r.Name}"/>
            </apex:column>
            <apex:column styleClass="tableRowData" headerValue="FY Value" headerClass="tableRowData">
                <apex:outputText value="$ {0,number,###,###,###,###}"><apex:param value="{!opp.Total_First_Year_Value__c}"/></apex:outputText>
            </apex:column>
            <apex:column styleClass="tableRow" headerValue="Reason" headerClass="tableRow">
                <apex:outputText value="{!opp.Reason_Won_or_Lost__c}"/>
            </apex:column>
            <apex:column styleClass="tableRowData" headerValue="Opportunity Closed Date" headerClass="tableRowData">
                <apex:outputText value="{0,date,dd/MM/yy}"><apex:param value="{!opp.Opportunity_Closed_Date__c}"/></apex:outputText>
            </apex:column>
        </apex:dataTable>
        <br/>
        
        <apex:dataTable value="{!myScorecard.openList}" var="opp" cellpadding="2" cellspacing="2" rowClasses="tableRow,tableRowBlue" styleClass="table3" headerClass="tableHeadGray">
            <apex:facet name="header"><apex:outputText value="Top 5 Opportunities in the Pipeline by First Year Value as of {0,date,MMMMM dd, yyyy}"><apex:param value="{!today()}"/></apex:outputText></apex:facet>
            <apex:column styleClass="tableRow" headerValue="Opportunity Name" headerClass="tableRow">
                <apex:outputText value="{!opp.Name}"/>
            </apex:column>
            <apex:column styleClass="tableRow" headerValue="Stage" headerClass="tableRow">
                <apex:outputText value="{!opp.StageName}"/>
            </apex:column>
            <apex:column styleClass="tableRowData" headerValue="Probability" headerClass="tableRowData">
                <apex:outputText value="{0,number,###}"><apex:param value="{!opp.Probability}"/></apex:outputText>%
            </apex:column>
            <apex:column styleClass="tableRowData" headerValue="FY Value" headerClass="tableRowData">
                <apex:outputText value="$ {0,number,###,###,###,###}"><apex:param value="{!opp.Total_First_Year_Value__c}"/></apex:outputText>
            </apex:column>
            <apex:column styleClass="tableRowData" headerValue="Expected Close Date" headerClass="tableRowData">
                <apex:outputText value="{0,date,dd/MM/yy}"><apex:param value="{!opp.CloseDate}"/></apex:outputText>
            </apex:column>
        </apex:dataTable>
    </div>
    <br/>

    <div class="div2">
        <table class="table2">
        <tr>
            <th class="tableHeadGray">Client Satisfaction Survey</th>
            <th class="tableHeadGray" align="center">{!prevYear}</th>
            <th class="tableHeadGray" align="center">{!financialYear}</th>
        </tr>
        <tr>
            <td class="tableRow">Likelihood to Recommend</td>
            <td class="tableRow" align="center">{!myScorecard.cssMap['PYlikelihood']}</td>
            <td class="tableRow" align="center">{!myScorecard.cssMap['CYlikelihood']}</td>
        </tr>
        <tr>
            <td class="tableRowBlue">Overall Satisfaction</td>
            <td class="tableRowBlue" align="center">{!myScorecard.cssMap['PYsatisfaction']}</td>
            <td class="tableRowBlue" align="center">{!myScorecard.cssMap['CYsatisfaction']}</td>
        </tr>
        </table>
    </div>
</div>

<div class="containerWithBreakBefore">
    <table class="table0">
        <tr>
            <td align="left"><apex:image url="{!URLFOR($Resource.Client_Scorecard, 'AonHewittLogo.PNG')}" width="100" height="27"/></td>
        </tr>
    </table>

    <div class="top">
        <table class="table0">
            <tr>
                <th align="left" class="title">{!financialYear} {!quarter} APAC Client Scorecard</th>
            </tr>
        </table>
        <hr class="hr"/>
    </div>
    
    <div class="div6">
        <table class="table3">
            <tr>
                <th colspan="6" class="tableHeadGray">Penetration</th>
                <th class="tableHeadGray" style="{!If(casListSize > 0, 'display:visible;' , 'display:none')}">Score<font style="font-size: 9.0pt;">&#42;</font> &nbsp;<apex:outputText value="{0,number,###}"><apex:param value="{!penetrationScore}"/></apex:outputText>%</th>
                <th class="tableHeadGray" style="{!If(casListSize == 0, 'display:visible;' , 'display:none')}">Score<font style="font-size: 9.0pt;">&#42;</font> &nbsp;0%</th>
            </tr>
            <tr>
                <td width="25%" class="tableRowRight">Service</td>
                <td align="center" width="12.5%" class="tableRowRight">Not Viable</td>
                <td align="center" width="12.5%" class="tableRowRight">Recent Loss</td>
                <td align="center" width="12.5%" class="tableRowRight">No Activity or N/A</td>
                <td align="center" width="12.5%" class="tableRowRight">Opportunity in SFDC</td>
                <td align="center" width="12.5%" class="tableRowRight">Partially Penetrated</td>
                <td align="center" width="12.5%" class="tableRow">Lead Provider</td>
            </tr>
            <apex:repeat var="cas" value="{!myScorecard.casList}">
            <tr>
                <td colspan="7" class="tableRowGray">Performance &amp; Talent</td>
            </tr>
            <tr>
                <td class="tableRowBottom">Performance and Organisation</td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Performance_and_Organisation__c == 'Not Viable', 'black', 'white')}"><apex:outputText value="" rendered="{!If(cas.Performance_and_Organisation__c == 'Not Viable', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Performance_and_Organisation__c == 'Recent Loss', '#696969', 'white')}"><apex:outputText value="" rendered="{!If(cas.Performance_and_Organisation__c == 'Recent Loss', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Performance_and_Organisation__c == 'No Activity or N/A', '#FF0000', 'white')}"><apex:outputText value="" rendered="{!If(cas.Performance_and_Organisation__c == 'No Activity or N/A', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Performance_and_Organisation__c == 'Opportunity in SFDC', '#00FF00', 'white')}"><apex:outputText value="" rendered="{!If(cas.Performance_and_Organisation__c == 'Opportunity in SFDC', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Performance_and_Organisation__c == 'Partially Penetrated', '#00BFFF', 'white')}"><apex:outputText value="" rendered="{!If(cas.Performance_and_Organisation__c == 'Partially Penetrated', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Performance_and_Organisation__c == 'Lead Provider', '#4876FF', 'white')}"><apex:outputText value="" rendered="{!If(cas.Performance_and_Organisation__c == 'Lead Provider', true, false)}" /></td>
            </tr>
            <tr>
                <td class="tableRowBottom">Talent and Engagement Consulting</td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Talent_and_Engagement_Consulting__c == 'Not Viable', 'black', 'white')}"><apex:outputText value="" rendered="{!If(cas.Talent_and_Engagement_Consulting__c == 'Not Viable', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Talent_and_Engagement_Consulting__c == 'Recent Loss', '#696969', 'white')}"><apex:outputText value="" rendered="{!If(cas.Talent_and_Engagement_Consulting__c == 'Recent Loss', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Talent_and_Engagement_Consulting__c == 'No Activity or N/A', '#FF0000', 'white')}"><apex:outputText value="" rendered="{!If(cas.Talent_and_Engagement_Consulting__c == 'No Activity or N/A', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Talent_and_Engagement_Consulting__c == 'Opportunity in SFDC', '#00FF00', 'white')}"><apex:outputText value="" rendered="{!If(cas.Talent_and_Engagement_Consulting__c == 'Opportunity in SFDC', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Talent_and_Engagement_Consulting__c == 'Partially Penetrated', '#00BFFF', 'white')}"><apex:outputText value="" rendered="{!If(cas.Talent_and_Engagement_Consulting__c == 'Partially Penetrated', true, false)}" /></td>
