            </tr>
            <tr>
                <td class="tableRowBottom">Engagement Surveys</td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Engagement_Surveys__c == 'Not Viable', 'black', 'white')}"><apex:outputText value="" rendered="{!If(cas.Engagement_Surveys__c == 'Not Viable', true, false)}" /></td>
                <td class="tableRowBottom" bgcolor="{!If(cas.Engagement_Surveys__c == 'Recent Loss', '#696969', 'white')}"><apex:outputText value="" rendered="{!If(cas.Engagement_Surveys__c == 'Recent Loss', true, false)}" /></td>
