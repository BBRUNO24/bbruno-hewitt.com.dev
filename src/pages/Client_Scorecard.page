<apex:page cache="false" title="Client Scorecard" standardController="Account" extensions="ctlr_Client_Scorecard" renderAs="PDF">
<apex:stylesheet value="{!URLFOR($Resource.Client_Scorecard, 'Client_Scorecard_StyleSheet.css')}" />
<head>
	<style type="text/css">
	@page {
    	counter-increment: page;
    	@bottom-right {
			font-family: Arial Sans-Serif;
			font-size: 9.0pt;
			font-style: italic;
        	content: "Page " counter(page) " of " counter(pages) "&nbsp;&nbsp;&nbsp;" 
    	}
    	@bottom-center {
			font-family: Arial Sans-Serif;
			font-size: 9.0pt;
			font-style: italic;
        	content: "Printed on &nbsp;<apex:outputText value="{0,date,MMMM dd, yyyy}"><apex:param value="{!today()}"/></apex:outputText>" 
    	}
    	@bottom-left {
			font-family: Arial Sans-Serif;
    		font-size: 9.0pt;
    		font-style: italic;
    		content: "&nbsp;&nbsp;&nbsp;{!myScorecard.accountName}";
    	}
	}
	td.twenty{background-color:#FF0000; border:none}
  	td.forty{background-color:#F76541; border:none}
  	td.sixty{background-color:#FFFF00; border:none}
  	td.eighty{background-color:#B1FB17; border:none}
  	td.hundred{background-color:#00FF00; border:none}
  	td.grey{background-color:#DDDDDD; border:none} 
	</style>
</head>
<div class="container">
	<table class="table0">
		<tr>
			<td align="left"><apex:image url="{!URLFOR($Resource.Client_Scorecard, 'AonHewittLogo.PNG')}" width="100" height="27"/></td>
		</tr>
	</table>

	<div class="top">
    	<table class="table0">
        	<tr>
	            <th align="left" class="title">{!financialYear} {!quarter} US Client Scorecard</th>
            	<th align="right" class="title">&nbsp;</th>
	    	</tr>
    	</table>
    	<hr class="hr"/>
	</div>
    
    <div class="div1">
        <table class="table1">
        <tr>
            <th class="tableHeadGray" colspan="2">Account Summary</th>
        </tr>
        <tr>
            <td width="45%" class="tableRow">Account</td>
            <td width="55%" class="tableRow"><apex:outputText value="{!myScorecard.accountName}"/></td>
        </tr>
        <tr>
            <td class="tableRowBlue">Market</td>
            <td class="tableRowBlue"><apex:outputText value="{!myScorecard.market}"/></td>
        </tr>
        <tr>
            <td class="tableRow">Account Executive</td>
            <td class="tableRow"><apex:outputText value="{!myScorecard.ownerName}"/></td>
        </tr>
        <tr>
            <td class="tableRowBlue">Executive Sponsor</td>
            <td class="tableRowBlue"><apex:outputText value="{!myScorecard.executiveSponsor}"/></td>
        </tr>
        <apex:repeat var="iat" value="{!myScorecard.iatList}">
        <tr style="{!IF(iat.Contact_Role__c == 'General Manager (HRO GM)', 'display:visible;' , 'display:none')}">
            <td class="tableRow">GM</td>
            <td class="tableRow">{!iat.Contact__r.Name}</td>
        </tr>
        </apex:repeat>
        <apex:repeat var="iat" value="{!myScorecard.iatList}">
        <tr style="{!IF(iat.Contact_Role__c == 'BGM (Business Group Manager)', 'display:visible;' , 'display:none')}">  
            <td class="tableRowBlue">BGM</td>
            <td class="tableRowBlue">{!iat.Contact__r.Name}</td>
        </tr>
        </apex:repeat>
        <tr>
            <td class="tableRow">Client Promise Required</td>
            <td class="tableRow"><apex:outputText value="{!If(account.Client_Promise_Required__c, 'Yes', 'No')}"/></td>
        </tr>
        <tr>
            <td class="tableRowBlue">Client Promise Score</td>
            <td class="tableRowBlue"><apex:outputText value="{!score} % for {!currYear}"/></td>
        </tr>
        </table>
        <br/>
        
        <table class="table1a">
        <tr>
            <th class="tableHeadGray" colspan="2">Overall Relationship</th>
        </tr>
